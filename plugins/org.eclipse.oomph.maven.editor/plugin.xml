<plugin>
   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated Maven NOT -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <category
            id="org.eclipse.oomph"
            name="%_UI_OomphWizard_category"/>
      <wizard
            id="org.eclipse.oomph.maven.presentation.MavenModelWizardID"
            name="%_UI_MavenModelWizard_label"
            class="org.eclipse.oomph.maven.presentation.MavenModelWizard"
            category="org.eclipse.oomph"
            icon="icons/full/obj16/MavenModelFile.gif">
         <description>%_UI_MavenModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated Maven -->
      <editor
            id="org.eclipse.oomph.maven.presentation.MavenEditorID"
            name="%_UI_MavenEditor_label"
            icon="icons/full/obj16/MavenModelFile.gif"
            extensions="maven"
            class="org.eclipse.oomph.maven.presentation.MavenEditor"
            contributorClass="org.eclipse.oomph.maven.presentation.MavenActionBarContributor">
      </editor>
   </extension>
</plugin>
